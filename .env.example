# Supabase Configuration (Required)
# Get these values from your Supabase dashboard: Settings â†’ API
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Service Role Key (Required for admin/server-side scripts only)
# Keep this secret! Never expose in client-side code
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# App Configuration (Optional)
VITE_APP_NAME=Wall-B
VITE_APP_DESCRIPTION=A modern bouldering community app
VITE_APP_URL=http://localhost:3000

# Feature Flags (Optional - defaults to false if not set)
VITE_ENABLE_CHAT=true
VITE_ENABLE_COMMUNITIES=true
VITE_ENABLE_GYMS=true
VITE_ENABLE_EVENTS=true

# Debug Mode (Optional)
VITE_DEBUG=false

# Analytics (Optional)
VITE_GA_TRACKING_ID=your-google-analytics-id
VITE_MIXPANEL_TOKEN=your-mixpanel-token
